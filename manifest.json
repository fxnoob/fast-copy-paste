{
   "name" : "Speedster",
   "version":"2.0",
   "permissions":["tabs","activeTab","webNavigation", "*://*/*","clipboardWrite", "clipboardRead","contextMenus"],
   "background":{
                "scripts":["cp.js"]
               },
  "browser_action": {
    "default_title": "Speedster",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
"icons": {
          "16": "images/gear.png"
           
        },
"content_scripts": [{
     "matches": ["<all_urls>"],
     "js": ["content_script.js"],
     "run_at": "document_idle"
      
  }],
  "manifest_version": 2

}
