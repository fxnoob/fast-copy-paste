{
   "name" : "Fast Copy Paste",
   "version":"2.0",
   "permissions":["tabs","storage","webNavigation","*://*/*","clipboardWrite"],
   "background":{
                "scripts":["background.js"]
               },
  "browser_action": {
    "default_title": "Fast Copy Paste",
    "default_icon": "gui/images/icon.png",
    "default_popup": "gui/popup.html"
  },
"icons": {
          "16": "gui/images/gear.png"
           
        },
"content_scripts": [{
     "matches": ["<all_urls>"],
     "js": ["jquery.min.js", "annyang.min.js","js/classie.js","js/cssParser.js","js/modalEffects.js","js/modernizr.custom.js","content_script.js"],
     "css":["css/component.css"],
     "run_at": "document_end"
  }], 
  "manifest_version": 2
}

