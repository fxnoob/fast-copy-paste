{
   "name" : "Fast Copy Paste",
   "version":"2.0",
   "permissions":["tabs","storage","webNavigation","*://*/*","clipboardWrite"],
   "background":{
                "scripts":["background.js"]
               },
  "browser_action": {
    "default_title": "Fast Copy Paste",
    "default_icon": "gui/images/icon.png",
    "default_popup": "gui/popup.html"
  },
"icons": {
          "16": "gui/images/gear.png"
           
        },
"content_scripts": [{
     "matches": ["<all_urls>"],
     "js": ["content_script.js"],
     "run_at": "document_idle"
      
  }],
  "manifest_version": 2

}
