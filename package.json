{
  "name": "speech-recognition-tool",
  "version": "4.1.6",
  "description": "voice commands for chrome browser",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss build src/styles/style.css -o dist/style.css",
    "watch:css": "yarn build:css && chokidar 'src/styles/*.css' -c 'yarn build:css'",
    "watch:js": "webpack --watch --config webpack.config.js --mode=development",
    "build:js": "webpack  --config webpack.config.js --mode=production",
    "rm": "rm -rf dist/",
    "start": "yarn dev",
    "dev": "concurrently --kill-others 'yarn watch:css' 'yarn watch:js'",
    "build": "yarn rm && concurrently 'yarn build:css' 'yarn build:js'",
    "jsdoc": "jsdoc src -r",
    "deploy_jsdoc": "yarn jsdoc &&  gh-pages -d out",
    "test": "yarn build",
    "generate:emoji": "node scripts/transform_emoji.js",
    "generate:locale": "node scripts/generate-locale",
    "generate:command": "node scripts/generate-command",
    "delete:command": "node scripts/delete-command",
    "delete:locale": "node scripts/delete-locale",
    "fix:prettier": "prettier --write $(git ls-files --others --exclude-standard)"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  },
  "bin": {
    "generate-command": "scripts/generate-command"
  },
  "author": "Hitesh Saini <fxnoob71@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "brotli-webpack-plugin": "^1.1.0",
    "chai": "^4.2.0",
    "chokidar": "^3.4.2",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "esbuild-loader": "^2.4.0",
    "eslint": "^7.6.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.5",
    "eslint-watch": "^7.0.0",
    "exec-sh": "^0.3.4",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "image-webpack-loader": "^5.0.0",
    "is-var-name": "^2.0.0",
    "jsdoc": "^3.6.3",
    "jsonfile": "^6.0.1",
    "mockery": "^2.1.0",
    "node-google-translate-skidz": "^1.1.2",
    "node-notifier": "^8.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0",
    "react-hot-loader": "^4.12.10",
    "style-loader": "^1.2.1",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "worker-loader": "^3.0.4"
  },
  "dependencies": {
